@model List<Instractor>
<table class="table table-hover table-bordered">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.InsName</td>
            <td><img src="~/Imgs/@item.InsImage" style="width:100px;height:80px" /></td>
            <td><a href="/Course/Detail/@item.Id" class="btn btn-primary">Details</a></td>
        </tr>
    }
</table>


